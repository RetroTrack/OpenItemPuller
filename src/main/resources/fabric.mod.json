{
  "schemaVersion": 1,
  "id": "openitempuller",
  "version": "${version}",
  "name": "OpenItemPuller",
  "description": "Wondering where you left all those items? With OIP you'll find them in seconds!",
  "authors": [],
  "contact": {
    "repo": "https://github.com/RetroTrack/OpenItemPuller"
  },
  "license": "MIT",
  "icon": "assets/openitempuller/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "com.retrotrack.openitempuller.client.ItemPullerClient"
    ],
    "main": [
      "com.retrotrack.openitempuller.ItemPuller"
    ]
  },
  "mixins": [
    "openitempuller.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
